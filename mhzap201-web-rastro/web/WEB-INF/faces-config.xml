        <managed-bean-name>RastroFuncionPar1</managed-bean-name>
        <managed-bean-class>com.egt.web.rastro.RastroFuncionPar1</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>GrupoProceso1</managed-bean-name>
        <managed-bean-class>com.egt.web.rastro.GrupoProceso1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>GrupoProceso2</managed-bean-name>
        <managed-bean-class>com.egt.web.rastro.GrupoProceso2</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/RastroFuncion1.jsp</from-view-id>
        <navigation-case>
            <from-outcome>parametros</from-outcome>
            <to-view-id>/RastroFuncionPar1.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>detalle</from-outcome>
            <to-view-id>/RastroFuncion2.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/RastroFuncion2.jsp</from-view-id>
        <navigation-case>
            <from-outcome>parametros</from-outcome>
            <to-view-id>/RastroFuncionPar1.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/GrupoProceso1.jsp</from-view-id>
        <navigation-case>
            <from-outcome>detalle</from-outcome>
            <to-view-id>/GrupoProceso2.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
