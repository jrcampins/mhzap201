        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FragmentoDetalle</managed-bean-name>
        <managed-bean-class>com.egt.web.rastro.FragmentoDetalle</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
