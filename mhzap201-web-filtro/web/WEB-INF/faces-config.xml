<?xml version='1.0' encoding='UTF-8'?>
<!-- =========== FULL CONFIGURATION FILE ================================== -->
<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <application>
        <resource-bundle>
            <base-name>com.egt.web.filtro.Bundle</base-name> 
            <var>bundle</var> 
        </resource-bundle> 
    </application>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FragmentoCabeza1</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FragmentoCabeza1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FragmentoCabeza2</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FragmentoCabeza2</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FragmentoCabeza3</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FragmentoCabeza3</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FragmentoColumnaIzquierda</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FragmentoColumnaIzquierda</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FragmentoDetalle</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FragmentoDetalle</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FragmentoFiltro</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FragmentoFiltro</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FragmentoFiltro_1</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FragmentoFiltro_1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FragmentoJoin</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FragmentoJoin</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FragmentoMenuVertical</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FragmentoMenuVertical</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FragmentoPie1</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FragmentoPie1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FragmentoPie2</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FragmentoPie2</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FragmentoPie3</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FragmentoPie3</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FiltroFuncion1</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FiltroFuncion1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FiltroFuncion2</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FiltroFuncion2</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>FiltroFuncionPar1</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FiltroFuncionPar1</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>FiltroFuncionPar2</managed-bean-name>
        <managed-bean-class>com.egt.web.filtro.FiltroFuncionPar2</managed-bean-class>
    
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <navigation-rule>
        <from-view-id>/FiltroFuncion1.jsp</from-view-id>
        <navigation-case>
            <from-outcome>detalle</from-outcome>
            <to-view-id>/FiltroFuncion2.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>criterios</from-outcome>
            <to-view-id>/FiltroFuncionPar1.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/FiltroFuncion2.jsp</from-view-id>
        <navigation-case>
            <from-outcome>criterios</from-outcome>
            <to-view-id>/FiltroFuncionPar1.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/FiltroFuncionPar1.jsp</from-view-id>
        <navigation-case>
            <from-outcome>detalle</from-outcome>
            <to-view-id>/FiltroFuncionPar2.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
